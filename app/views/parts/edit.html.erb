<div class="container">
  <div class="row">
    <div>
      <h4>トレーニング部位</h4>
      <%= form_with model: @part, url: part_path, local: true do |f| %>
      <div class="form-group">
        <%= f.label :部位名 %>
        <%= f.text_field :part %>
        <%= f.submit "変更を保存", class: "btn btn-success" %>
      </div>
      <% end %>
    </div>
  </div>
</div>
